{
  "name": "@mooore/stylelint-config",
  "version": "2.0.1",
  "description": "A shareable stylelint config object that enforces Mooore css rules",
  "homepage": "https://mooore.nl/",
  "author": "Mooore (https://github.com/mooore)",
  "license": "MIT",
  "keywords": [
    "mooore",
    "fylgja",
    "stylelint",
    "stylelint-config",
    "magento",
    "hyva"
  ],
  "main": "index.js",
  "scripts": {
    "test:luma": "stylelint _tests_/index.less --config luma.js",
    "test:snowdog": "stylelint _tests_/index.scss --config snowdog.js",
    "test:hyva": "stylelint _tests_/hyva.phtml --config index.js",
    "test:scss": "stylelint _tests_/index.scss --config scss.js",
    "test:main": "stylelint _tests_/index.{css,phtml,vue} --config index.js",
    "test": "npm run test:luma && npm run test:snowdog && npm run test:hyva && npm run test:scss && npm run test:main"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mooore-digital/stylelint-config"
  },
  "dependencies": {
    "@fylgja/stylelint-config": "^5.0.1",
    "postcss-less": "^6.0.0",
    "postcss-scss": "^4.0.6",
    "stylelint-order": "^6.0.1"
  },
  "devDependencies": {
    "postcss": "^8.4.21",
    "stylelint": "^14.15.0"
  },
  "peerDependencies": {
    "stylelint": "^14.14.0",
    "postcss": "8.4.21"
  }
}
